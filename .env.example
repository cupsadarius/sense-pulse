# Redis
REDIS_URL=redis://redis:6379

# Docker
COMPOSE_PROJECT_NAME=sense-pulse

# Pi-hole
PIHOLE_HOST=http://pi.hole
PIHOLE_PASSWORD=

# Weather
WEATHER_LOCATION=

# Aranet4
ARANET4_SENSORS=[]
